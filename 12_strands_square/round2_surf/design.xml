      <MutateResidue name="Asn137" target="137A" new_res="ASN" />
      <MutateResidue name="Asn167" target="167A" new_res="ASN" />

    </MOVERS>
    <APPLY_TO_POSE>
    </APPLY_TO_POSE>
    <PROTOCOLS>
      <Add mover_name="Tyr15" />
      <Add mover_name="Tyr29" />
      <Add mover_name="Tyr41" />
      <Add mover_name="Trp43" />
      <Add mover_name="Tyr57" />
      <Add mover_name="Tyr71" />
      <Add mover_name="Tyr87" />
      <Add mover_name="Tyr101" />
      <Add mover_name="Trp103" />
