      <Add mover_name="Tyr133" />
      <Add mover_name="Tyr147" />
      <Add mover_name="Tyr159" />
      <Add mover_name="Trp161" />
      <Add mover_name="Tyr175" />
      <Add mover_name="Asn19" />
      <Add mover_name="Asn49" />
      <Add mover_name="Asn75" />
      <Add mover_name="Asn109" />
      <Add mover_name="Asn137" />
      <Add mover_name="Asn167" />

      <Add mover_name="fdesign" />
      
      Add filter=holes/>
      Add filter=packstat/>
      Add filter=geo/>
