      <MutateResidue name="Tyr119" target="119A" new_res="TYR" />
      <MutateResidue name="Tyr133" target="133A" new_res="TYR" />
      <MutateResidue name="Tyr147" target="147A" new_res="TYR" />
      <MutateResidue name="Tyr159" target="159A" new_res="TYR" />
      <MutateResidue name="Trp161" target="161A" new_res="TRP" />
      <MutateResidue name="Tyr175" target="175A" new_res="TYR" />
      <MutateResidue name="Asn19" target="19A" new_res="ASN" />
      <MutateResidue name="Asn49" target="49A" new_res="ASN" />
